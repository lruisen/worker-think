{
    "name": "lruisen/worker-think",
    "description": "Starting HTTP|WS Service through Workerman on Thinkphp8.0",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "WorkerThink\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Lruisen",
            "email": "lruisen@email.cn"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "^8.0",
        "workerman/gateway-worker": "^3.1",
        "voku/anti-xss": "^4.1"
    },
    "require-dev": {
        "symfony/var-dumper": ">=4.2"
    },
    "extra": {
        "think": {
            "services": [
                "WorkerThink\\Service"
            ],
            "config": {
                "worker_process": "src/config/worker_process.php",
                "worker_http": "src/config/worker_http.php",
                "worker_ws": "src/config/worker_ws.php"
            }
        }
    }
}
